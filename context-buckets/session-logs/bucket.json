{
  "id": "session-logs",
  "name": "Session Logs",
  "description": "Persistent session memory files created by orchestrators and the /save command. Captures key findings, decisions, research, and open items from each session to enable continuity across conversations.",
  "version": "1.0",
  "created": "2026-02-04",

  "content_type": "documents",

  "sources": [
    {
      "type": "local",
      "path": "files/",
      "file_types": ["md"],
      "organization": "date-based",
      "naming_convention": "YYYY-MM-DD_topic-slug.md"
    }
  ],

  "access_control": {
    "default_level": "read-write",
    "allowed_agents": ["personal-assistant"],
    "allowed_teams": ["wharfside-board-assistant", "software-project", "altium-solutions", "youtube-content", "personal-assistant"]
  }
}

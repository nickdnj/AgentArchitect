{
  "id": "youtube-content",
  "name": "YouTube Content Team",
  "description": "Video production and multi-platform short-form content strategy.",
  "version": "1.0",
  "created": "2026-02-11",

  "members": [
    {
      "agent_id": "youtube-creator",
      "role": "Full-length video production - script, assets, assembly, upload",
      "context_override": null
    },
    {
      "agent_id": "short-form-video",
      "role": "Shorts/Reels/TikTok extraction and posting strategy",
      "context_override": null
    }
  ],

  "shared_context": {
    "buckets": ["ai-journey"],
    "outputs_folder": "teams/youtube-content/outputs"
  },

  "orchestration": {
    "mode": "thin-orchestrator",
    "execution": { "model": "opus", "max_turns": 50 },
    "routing": {
      "full-video": ["youtube-creator"],
      "shorts-reels": ["short-form-video"],
      "general": ["youtube-creator"]
    },
    "delegation_strategy": "Route video production to youtube-creator. After full video is complete, route to short-form-video for clip extraction. Sequential workflow.",
    "session_summary": {
      "enabled": true,
      "output_path": "context-buckets/session-logs/files/"
    }
  },

  "collaboration_rules": {
    "coordination_mode": "orchestrated",
    "handoff_protocol": "sequential-delegation",
    "output_sharing": "full",
    "notification_email": "nickd@demarconet.com"
  }
}
